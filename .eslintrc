{
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src", "./"]
      }
    }
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "sort-destructure-keys", "sort-keys-fix"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 100
      }
    ],
    "sort-destructure-keys/sort-destructure-keys": 2,
    "sort-keys-fix/sort-keys-fix": 2
  }
}
